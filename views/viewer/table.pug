extends ../layout

block content
    include navbar

    .container
        h1= title
    
        table.table.table-striped.table-hover.table-condensed
            thead
                tr
                    th Type
                    th User
                    th Parking
                    th Message
                    th Timestamp
            tbody
                each log in logs
                    tr
                        td= log.type
                        td #{log.id} ï½œ #{log.userId}
                        td= log.parkings || '-'
                        td= log.action
                        td= log.timestamp